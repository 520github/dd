}
.switch-list {
	padding-left: 20px;
}
.switch-list ul {
	width: 940px;
	height: 87px;
}
.switch-list ul li {
	width: 216px;
	line-height: 87px;
	float: left;
	margin-right: 5px;
	text-indent: -999em;
	overflow: hidden;
}
.switch-list ul li img {
	height: 378px;
}
.switch-list ul li.switch-list-1:hover, .switch-btn-1 {
	background: url(../images/recommendBtn.png) 0 -87px;
}
.switch-list ul li.switch-list-2:hover, .switch-btn-2 {
	background: url(../images/recommendBtn.png) -221px -87px;
}
.switch-list ul li.switch-list-3:hover, .switch-btn-3 {
	background: url(../images/recommendBtn.png) -442px -87px;
}
.switch-list ul li.switch-list-4:hover, .switch-btn-4 {
	background: url(../images/recommendBtn.png) -663px -87px;
}
.floating-layer {
	top: 0px;
	position: absolute;
	margin: 40px 0 0 0;
}
.floating-layer-1 {
	margin-left: 580px;
}
.floating-layer h2, .floating-layer h3, .floating-layer h4{
	font-weight: 500;
}
.floating-layer h2 {
	color: #0459a5;
	font-size: 38px;
	line-height: 80px;
	text-shadow: 0 1px 1px #FFF;
	-webkit-text-shadow: 0 1px 1px #FFF;
	-moz-text-shadow: 0 1px 1px #FFF;
}
.floating-layer h3, .floating-layer h4 {
	color: #007fb8;
	font-size: 18px;
	line-height: 30px;
	margin: 10px 0 10px 0;
}
.pages-4 .floating-layer h3 {
	font-size:16px;
}
/* .floating-layer h4 {
	color: #fff;
	font-size: 18px;
	line-height: 30px;
} */
.floating-layer ul li {
	color: #0459a6;
