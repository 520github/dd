	font-weight: bold;
	background-position: -8px -242px;
	padding-left: 20px;
}
.floating-layer .btn a {
	display: inline-block;
	height: 47px;
	line-height: 47px;
	font-size: 20px;
	text-align: center;
	margin: 20px 5px 0 0;
