}
.floating-layer .btn a.understand-btn {
	background-position: -1px -185px;
	width: 131px;
}
.floating-layer .btn a.register-btn {
	background-position: -135px -185px;
	width: 92px;
}
.floating-layer .btn a.iphome-btn {
	background-position: -238px -185px;
	width: 92px;
	width: 176px;
	height: 50px;
}
/*================顶部幻灯片1================ */
.slideshow, #recommend {
	width: 100%;
	position: absolute;
	height: 466px;
	left: 0;
	background: #c1effa; /* Old browsers */
	background: -moz-linear-gradient(top, #c1effa 0%, #ade4f3 30%, #81cbe3 54%, #81cbe3 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #c1effa), color-stop(30%, #ade4f3), color-stop(54%, #81cbe3), color-stop(100%, #81cbe3)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #c1effa 0%, #ade4f3 30%, #81cbe3 54%, #81cbe3 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, #c1effa 0%, #ade4f3 30%, #81cbe3 54%, #81cbe3 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #c1effa 0%, #ade4f3 30%, #81cbe3 54%, #81cbe3 100%); /* IE10+ */
	background: linear-gradient(to bottom, #c1effa 0%, #ade4f3 30%, #81cbe3 54%, #81cbe3 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c1effa', endColorstr='#81cbe3', GradientType=0 ); /* IE6-9 */
}
.slideshowA .left-right-slideshow {
