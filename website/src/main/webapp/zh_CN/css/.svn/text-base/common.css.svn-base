.size16{ font-size:16px;}
.size15{ font-size:15px;}
.size17{ font-size:17px;}
.size18{ font-size:18px;}
.size22{ font-size:22px;}
.size24{ font-size:24px;}
.size30{ font-size:30px;}

/*==========================  颜色 ========================== */
.color1{color:#333;}
.color2{color:#666;}
.color3{color:#999;}
.color4{color:#0690cf;}
.color5{color:#66d2ff;}
.color6{color:#fff;}
