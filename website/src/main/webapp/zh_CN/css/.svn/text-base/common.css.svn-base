.color8{color:#646464;}

/*==========================  行高 ========================== */
.lh20{line-height:20px;}
.lh22{line-height:22px;}
.lh24{line-height:24px;}
.lh25{line-height:25px;}
.lh28{line-height:28px;}
.lh30{line-height:30px;}
.lh35{line-height:35px;}
.lh40{line-height:40px;}
.lh50{line-height:50px;}
.lh60{line-height:60px;}
.lh70{line-height:70px;}

/*==========================   字体粗细========================== */
.weight9{ font-weight:900;}
.weight5{ font-weight:500;}

/*========================== 左右========================== */
.lt{float:left;}
.rt{float:right;}

.radius{
border-radius:5px;  -moz-border-radius:5px;  -webkit-border-radius:5px;  border:1px solid #d9d9d9;
}
.border{border:1px solid #d9d9d9;}
.gradient-x{ background:url(../images1/gradient-x.png) repeat-x; height:200px;}

.login{margin:0 auto; width:960px;}

input[type=text], input[type=password] {
	border-radius:5px;  -moz-border-radius:5px;  -webkit-border-radius:5px;  border:1px solid #dbdbdb; height:34px;
	background: -moz-linear-gradient(top,  #ffffff 10%, ##F2F2F2 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(10%,#ffffff), color-stop(100%,##F2F2F2)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #ffffff 10%,##F2F2F2 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #ffffff 10%,##F2F2F2 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #ffffff 10%,##F2F2F2 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #ffffff 10%,##F2F2F2 100%); /* W3C */
	/*filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='##F2F2F2',GradientType=0 ); IE6-9 */
	padding:0 10px;
}
input[type=submit]{
	border:0; overflow:hidden; text-indent:-9999px; display:block;
}

#register{ margin:0 auto;}



