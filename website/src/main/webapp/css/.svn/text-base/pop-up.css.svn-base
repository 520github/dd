.scrollbar-list { width: 100%; clear: both; margin: 20px 0 10px; }
.scrollbar-list .viewport { width: 99%; height: 230px; overflow: hidden; position: relative; 	border-bottom: 1px dashed #E2DED9;}
.scrollbar-list  .overview { list-style: none; position: absolute; left: 0; top: 0; }

.scrollbar-list  .thumb .end,
.scrollbar-list  .thumb { background-color: #e6e6e6; 	-moz-border-radius: 20px; -khtml-border-radius: 20px; -webkit-border-radius: 20px; border-radius: 10px;	}
.scrollbar-list  .scrollbar { position: relative; float: right; width: 5px; }
.scrollbar-list  .track { height: 100%; width:13px; position: relative; padding: 0 1px; }
.scrollbar-list  .thumb { height: 20px; width: 5px; cursor: pointer; overflow: hidden; position: absolute; top: 0; }
.scrollbar-list  .thumb .end { overflow: hidden; height: 5px; width: 13px; }
.scrollbar-list  .disable{ display: none; }


