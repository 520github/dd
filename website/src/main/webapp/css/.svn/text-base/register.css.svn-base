.fans-list h2 a.add-btn{ background-position:-1380px -202px;  width:95px; height:28px; margin-top:3px; text-align:center; text-shadow: 0px 1px 1px #fff; -moz-text-shadow: 0px 1px 1px #fff; -webkit-text-shadow: 0px 1px 1px #fff;}
.fans-list .list {overflow:hidden; margin-bottom:30px; width: 100%;}
.fans-list .list li{ width:236px; height:54px;  float:left; border-radius:2px; -moz-border-radius:2px; -webkit-border-radius:2px; padding:7px 4px 0 12px; margin-top:15px;}
.fans-list .list li img{ width:50px; height:50px; float:left; border-radius:2px; -moz-border-radius:2px; -webkit-border-radius:2px;  margin-right:10px;}
.fans-list .list li:hover{ background:#f3f3f3;}
.fans-list .list li b a.add-btn{background-position:-1303px -202px;  color:#fff; float:right; width:70px; height:28px;  text-align:center; text-shadow: 1px 1px 1px #638f3f; -moz-text-shadow: 1px 1px 1px #638f3f; -webkit-text-shadow: 1px 1px 1px #638f3f;}

.complete-btn{ width:100%; }
